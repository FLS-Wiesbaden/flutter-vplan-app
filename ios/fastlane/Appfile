app_identifier(ENV['MATCH_APP_IDENTIFIER_IOS']) # The bundle identifier of your app
apple_id(ENV['FASTLANE_USER']) # Your Apple Developer Portal username

itc_team_id(ENV['CI_APPLE_ITC_TEAM_ID']) # App Store Connect Team ID
team_id(ENV['CI_APPLE_TEAM_ID']) # Developer Portal Team ID

